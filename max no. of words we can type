class Solution {
    public int canBeTypedWords(String text, String brokenLetters) {
        String words[] = text.split(" ");
       int count = 0;
       for(String str : words){
        for(char c : str.toCharArray()){
            if(brokenLetters.indexOf(c)!=-1){  count++;
            break;}
          
        }
         
       }
       return(words.length - count );
        
    }
}
