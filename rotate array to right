class Solution {
    public void rotate(int[] nums, int k) {
        int l = nums.length;
        k=k%l;
        int ans [] = new int[l];
        for(int i = 0; i < k ; i++)
           ans[i] = nums[l-k+i];
        
        for(int j = 0 ; k+j<l;j++)
          ans [k+j]= nums[j];
    
        for(int n =0 ; n <l ; n++){
           nums[n]= ans[n];
        }
    }
}
