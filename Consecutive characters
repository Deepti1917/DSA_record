class Solution {
    public int maxPower(String s) {
     
     int x=1;
     int ans =0;
     if(s.length()==1)
     return 1;
     for(int i = 1; i <s.length();i++){
    
        if(s.charAt(i)==s.charAt(i-1))
        x++;
        else
        x=1;
        ans = Math.max(ans,x);
     }
      return ans;

        
     }

    }
